Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"power", "[", 
      RowBox[{"ell_", ",", "chi_"}], "]"}], ":=", 
     RowBox[{"Pg", "[", 
      RowBox[{"ell", ",", "chi"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"integrand", "[", 
      RowBox[{"ell_", ",", "chi_"}], "]"}], ":=", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"W", "[", "chi", "]"}], "/", "chi"}], ")"}], "^", "2"}], 
      RowBox[{"power", "[", 
       RowBox[{"ell", ",", "chi"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"CgT", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"l", ",", "1", ",", 
         RowBox[{"Length", "@", "ellT"}]}], "}"}]}], "]"}]}], ";"}], "  ", 
   RowBox[{"(*", " ", 
    RowBox[{"Output", " ", "table"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Do", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"ell", "=", 
        RowBox[{"ellT", "[", 
         RowBox[{"[", "l", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", 
       RowBox[{"chiminell", "=", 
        RowBox[{"chimin", "[", "ell", "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"chimaxell", "=", 
        RowBox[{"Min", "[", 
         RowBox[{"chiMax", ",", 
          RowBox[{"chimax", "[", "ell", "]"}]}], "]"}]}], ";", 
       RowBox[{"(*", " ", 
        RowBox[{"Max", " ", "chi", " ", "in", " ", "integration"}], " ", 
        "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"Nchi", "=", "100"}], ";", " ", 
       RowBox[{"(*", " ", 
        RowBox[{
        "Number", " ", "of", " ", "chi", " ", "points", " ", "in", " ", "the",
          " ", 
         RowBox[{"integration", "."}]}], "  ", "*)"}], "\[IndentingNewLine]", 
       
       RowBox[{"deltachi", "=", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"chimaxell", "-", "chiminell"}], ")"}], "/", 
         RowBox[{"(", 
          RowBox[{"Nchi", "-", "1"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
        "Array", " ", "of", " ", "chi", " ", "points", " ", "for", " ", 
         "trapezoidal", " ", 
         RowBox[{"integration", ":"}]}], " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"chiT", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"chiminell", "+", 
           RowBox[{"deltachi", "*", 
            RowBox[{"(", 
             RowBox[{"i", "-", "1"}], ")"}]}]}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", "Nchi"}], "}"}]}], "]"}]}], ";", " ", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"pg", "=", "0"}], ";", 
       RowBox[{"pgB", "=", "0"}], ";", "\[IndentingNewLine]", 
       RowBox[{"chiA", "=", 
        RowBox[{"chiT", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"pgA", "=", 
        RowBox[{"integrand", "[", 
         RowBox[{"ell", ",", "chiA"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"Do", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"chiB", "=", 
           RowBox[{"chiT", "[", 
            RowBox[{"[", "i", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"pgB", "=", 
           RowBox[{"integrand", "[", 
            RowBox[{"ell", ",", "chiB"}], "]"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"deltachi", "=", 
           RowBox[{"chiB", "-", "chiA"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"pg", "=", 
           RowBox[{"pg", "+", 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"pgA", "+", "pgB"}], ")"}], "/", "2"}], "  ", "*", 
             "deltachi"}]}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"chiA", "=", "chiB"}], ";", "\[IndentingNewLine]", 
          RowBox[{"pgA", "=", "pgB"}], ";"}], "\[IndentingNewLine]", ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "2", ",", 
           RowBox[{"Length", "@", "chiT"}]}], "}"}]}], "]"}], ";", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"CgT", "[", 
         RowBox[{"[", "l", "]"}], "]"}], "=", 
        RowBox[{"{", 
