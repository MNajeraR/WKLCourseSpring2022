 RowBox[{
  RowBox[{"sysDplus", "=", 
   RowBox[{"NDSolve", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"Df", "''"}], "[", "eta", "]"}], " ", "+", 
         RowBox[{
          RowBox[{"f1", "[", "eta", "]"}], 
          RowBox[{
           RowBox[{"Df", "'"}], "[", "eta", "]"}]}], "-", 
         RowBox[{
          RowBox[{"f2", "[", "eta", "]"}], " ", 
          RowBox[{"Df", "[", "eta", "]"}]}]}], "\[Equal]", "0"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"Df", "'"}], "[", "etaini", "]"}], "\[Equal]", "dDplusi"}], 
       ",", 
       RowBox[{
        RowBox[{"Df", "[", "etaini", "]"}], "\[Equal]", "Dplusi"}]}], "}"}], 
     ",", "Df", ",", 
     RowBox[{"{", 
      RowBox[{"eta", ",", "etaini", ",", "etafI"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"DplusPre", "[", "eta_", "]"}], ":=", 
    RowBox[{
     RowBox[{"Evaluate", "[", 
      RowBox[{
       RowBox[{"Df", "[", "eta", "]"}], "/.", "sysDplus"}], "]"}], "[", 
     RowBox[{"[", "1", "]"}], "]"}]}], ";"}], " "}], "\[IndentingNewLine]", 
 RowBox[{
