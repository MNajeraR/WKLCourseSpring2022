       RowBox[{"2.", 
        RowBox[{"(", 
         RowBox[{"1.", "+", 
          RowBox[{
           FractionBox[
            RowBox[{"(", 
             RowBox[{"1", "-", "OmM"}], ")"}], "OmM"], 
           RowBox[{"Exp", "[", 
            RowBox[{"3.", "eta"}], "]"}]}]}], ")"}]}]]}], ")"}]}], ";"}], 
  RowBox[{"(*", 
